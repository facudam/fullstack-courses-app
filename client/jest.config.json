{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.tsx?$": "ts-jest",
    "^.+\\.module\\.(css|sass|scss)$": "jest-css-modules-transform"
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "moduleNameMapper": {
    "\\.module\\.css$": "identity-obj-proxy"
  }
}



  